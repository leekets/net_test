
@{
    ViewBag.Title = "TestList";
    string baseUrl = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/') + "/";
}
<script>
    var baseurl = "@baseUrl"+"test/";
</script>
@Html.Partial("_testHead")
<script src="~/Scripts/test/test_practice.js" type="text/javascript"></script>
<h2></h2>
@Html.Partial("_nav")

<div class="container" style="clear:both;">
    <div id="test">
        <ul class="list-group">
            <li class="sharewith list-group-item e_element e_act0"> <a class="pNum font-danger">Practice </a> </li>
            <!-- 问题_句2子 -->
            <li class="sharewith list-group-item e_element e_act0 e_act1 e_act2 e_act3"> <a class="sen"> </a> </li>
            <!-- 问题_句子 -->
            <li class="sharewith list-group-item e_element"> <a class="q_en"> </a> </li>
            <!-- 问题 -->
            <li class="sharewith list-group-item e_element"> <a class="q"> </a> </li>
            <!-- hint -->
            <li class="sharewith list-group-item a e_element e_act3"> <a class="hint"> </a> </li>
            <!-- 答案 -->
            <li class="sharewith list-group-item a e_element e_act0"> <a class="a"> </a> </li>
            <!-- 错误 -->
            <li class="sharewith list-group-item e e_element e_act3"> <a class="e font-danger"> </a> </li>
            <li class="sharewith list-group-item cn e_element"> <a class="cn"> </a> </li>
            <!-- 英文例句 -->
            <li class="sharewith list-group-item en e_element"> <a class="en"> </a> </li>
        </ul>
        <!-- 输入框 -->
        <div class="lib txtr e_element e_act0 e_act1 e_act2" style="border:1px solid #dddddd; padding:8px 12px; margin-bottom:18px;">
            <textarea name="message" class="b textarea_full max-width-none" id="mainInput" style="color:#f00"></textarea>
        </div>
    </div>
    <!-- -->

    <div class="mbm" id="btnPart">
        <a class="btn-primary btn-large e_element e_act0 btn0" href="javascript:void(0);">Practice</a>
        <a class="btn1 updatebtn e_element e_act1 btnQ btn-primary" href="javascript:void(0);">Submit</a>
        <a class="btn2 updatebtn e_element btn-primary" href="javascript:void(0);">Submit</a>
        <a class="btn3 updatebtn e_element e_act2 btn-primary">Submit</a>
        <!-- 按键：播放 -->
        <!--<div>
            <a class="updatebtn e_element e_act1 e_act2 btn btn-primary btn-large" onClick="document.getElementById('player').currentTime = 0;document.getElementById('player').play()" href="javascript:void(0);">Play</a>
        </div>-->
        <!-- 显示问题 -->
        <a class="btnCn updatebtn e_element btn-primary" href="javascript:void(0);">Show Question</a>
    </div>
    <!-- 音频 -->
    <audio class="e_element" id="player" preload="auto"></audio>
    <span id="lenMax" class="label label-info"></span> <span id="lenChange" class="label label-info"></span>
</div>

<a href="javascript:void(0);" accesskey="s"></a>
<script src="~/Scripts/test/ko_jump.js"></script>

